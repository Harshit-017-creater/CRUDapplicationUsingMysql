<table class="table table-striped table-responsive">
<thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>phone number</th>
        <th>password</th>
        <th></th>
    </tr>
</thead>
<tbody>
    {{#each list}}
    <tr>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.pno}}</td>
        <td>{{this.password}}</td>
        <td><a href="updateEmp/{{this.id}}" class="btn btn-primary">Edit</a>
        <a href="deleteEmp/{{this.id}}"class="btn btn-danger">Delete</a></td>
    </tr>
    {{/each}}
</tbody>
</table>