<table class="table table-striped table-responsive">
<thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>phone number</th>
        <th>password</th>
        <th></th>
    </tr>
</thead>
<tbody>
    {{#each list}}
    <tr>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.pno}}</td>
        <td>{{this.password}}</td>
    </tr>
    {{/each}}
</tbody>
</table>